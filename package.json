{
  "name": "preact-mobx-starter",
  "version": "0.0.1",
  "description": "Preact starter with preact-router and mobx-preact.",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "better-npm-run build",
    "build:dev": "better-npm-run build:dev",
    "start": "better-npm-run start",
    "prod": "better-npm-run prod"
  },
  "betterScripts": {
    "build": {
      "command": "npm run clean && webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:dev": {
      "command": "npm run clean && webpack",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start": {
      "command": "babel-node build/dev-server.js",
      "env": {
        "NODE_ENV": "development",
        "sslport": "8443",
        "port": "8080"
      }
    },
    "prod": {
      "command": "babel-node build/dev-server.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "author": "ditrix1993@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@vkontakte/vk-connect": "^1.6.5",
    "autobind-decorator": "^2.4.0",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "core-js": "^3.1.4",
    "dayjs": "^1.8.16",
    "mobx": "^5.13.0",
    "mobx-preact": "^3.0.0",
    "normalize.css": "^8.0.1",
    "preact": "^8.4.2",
    "preact-router": "^2.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "better-npm-run": "0.1.1",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.1.0",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-config-standard-react": "^8.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.17.1",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-style-loader": "^5.1.0",
    "json-loader": "^0.5.7",
    "mobx-preact-devtools": "0.0.19",
    "pixrem": "^5.0.0",
    "postcss-import": "^12.0.1",
    "postcss-import-alias-resolver": "^0.1.1",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.2",
    "postcss-scss": "^2.0.0",
    "preact-async-route": "^2.2.1",
    "preact-svg-loader": "^0.2.1",
    "precss": "^4.0.0",
    "rimraf": "^2.6.3",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-loader": "^0.23.1",
    "terser": "^3.14.1",
    "terser-webpack-plugin": "^1.4.1",
    "url-loader": "^2.1.0",
    "webpack": "^4.38.0",
    "webpack-bundle-analyzer": "^3.4.1",
