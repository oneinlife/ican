{
  "presets": [
    [
      "@babel/env",
      {
        "loose": true,
        "modules": false,
        "useBuiltIns": "entry",
        "corejs": { "version": 3, "proposals": true },
        "targets": {
          "esmodules": true,
        },
      }
    ],
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    ["@babel/plugin-transform-react-jsx", { "pragma": "h" }]
  ]
}